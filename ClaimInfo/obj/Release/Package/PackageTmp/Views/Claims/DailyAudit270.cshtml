
@{
    ViewBag.Title = "DailyAudit270";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    string ErrorCount = ViewBag.ErrorCount;
    string Tran_type = ViewBag.error_claimcount;
}

<div class="rightpart">
    <div class="dashBx">
        <div class="clearfix"></div>
        <h2>Eligibility Error Details </h2>
        <hr />
