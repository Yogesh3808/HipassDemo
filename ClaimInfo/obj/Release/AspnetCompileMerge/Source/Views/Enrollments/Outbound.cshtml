                    <th>Plan Integration Desc <img src="~/Images/search_3_16.ico" style="float: right" /></th>
                </tr>
                <tbody>

                    @foreach (var item in Model.objVisitDetailsList)
                    {
                    <tr>
                        <td>@item.Error_Type </td>
                        <td>@item.SFHPID </td>
                        <td>@item.CIN </td>
                        <td>@item.Inbound_X12_status </td>
