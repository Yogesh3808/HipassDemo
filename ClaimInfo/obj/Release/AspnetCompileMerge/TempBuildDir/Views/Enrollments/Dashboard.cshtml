        margin-top: 10px;
    }

    .inline {
        float: left;
        width: 33%;
    }

    * {
        box-sizing: border-box;
    }

    /* Create four equal columns that floats next to each other */
    .column {
        float: left;
        width: 25%;
    }

    /* Clear floats after the columns */
    .row:after {
        content: "";
        display: table;
        clear: both;
    }

    .bar {
        width: 50px;
        background: rgb(19, 157, 201);
    }

    .bar1 {
        width: 50px;
        background: #ec6236;
    }
</style>
