@model ClaimInfo.Models.ClaimsDailyAuditViewModel
@{
    ViewBag.Title = "ClaimsAudit";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
    int FileCount = ViewBag.FileCount;
    int Count999 = ViewBag.Count999;
}
<link href="~/CSS/StyleSheetForTable.css" rel="stylesheet" />
<div class="rightpart">
    <div class="dashBx">
        <div class="clearfix"></div>
        <div style="border-bottom: 1.5px solid #139DC9;">
            <img src="~/Images/Search.png" style="width:14px; height:14px;" />
            <input type="text" id="MyCustomSearch" style="color: #139DC9; font-weight: bold;" placeholder="Search" class="mySearchbox" name="min">
        </div>
        <br />
        <h2>Claims Audit</h2>
        <div class="dashformBx  dashformBx2 clearfix">
            <div>
                <div style="width:25%; display:none; float:left;">
                    <label class="lblTxt">Select Client Name </label>
                    @Html.DropDownList("ClientName", new List<SelectListItem>
                    {
                        new SelectListItem {Text = "Client Name 1", Value = "1",Selected = true},
                        new SelectListItem {Text = "Client Name 2", Value = "2"}
                    }, "Select Client Name", new {@class = "dashInp"})
                </div>
                <div class="dashfrm-L">
                    <label class="lblTxt">Select State </label>
                    @Html.DropDownList("State", new List<SelectListItem>
                    {
                        new SelectListItem {Text = "California", Value = "1" ,Selected = true},
                        new SelectListItem {Text = "Michigan", Value = "2"},
                     new SelectListItem {Text = "Florida", Value = "3"},
                        new SelectListItem {Text = "New York", Value = "4"},
                     new SelectListItem {Text = "Idaho", Value = "5"},
                        new SelectListItem {Text = "Ohio", Value = "6"},
                     new SelectListItem {Text = "Illinois", Value = "7"},
                        new SelectListItem {Text = "Texas", Value = "8"},
                   new SelectListItem {Text = "Mississippi", Value = "9"},
                        new SelectListItem {Text = "South Carolina", Value = "10"},
                    new SelectListItem {Text = "New Mexico", Value = "11"},
                        new SelectListItem {Text = "Puerto Rico", Value = "12"},
                              new SelectListItem {Text = "Washington", Value = "13"},
                    new SelectListItem {Text = "Utah", Value = "14"},
                        new SelectListItem {Text = "Wisconsin", Value = "15"}
                    }, "Select State", new {@class = "dashInp"})
                </div>
                <div class="dashfrm-M">
                    <label class="lblTxt">Select Trading Partner </label>
                    @Html.DropDownList("TradingPartner", new List<SelectListItem>
                    {
                        new SelectListItem {Text = "Trading Partner 1", Value = "1",Selected = true},
                        new SelectListItem {Text = "Trading Partner 2", Value = "2"}
                    }, "Select Trading Partner", new {@class = "dashInp"})
                </div>
                <div class="dashfrm-M">
                    <label class="lblTxt">Select Provider Name </label>
                    @Html.DropDownList("ProviderName", new List<SelectListItem>
                    {
                        new SelectListItem {Text = "Provider Name 1", Value = "1",Selected = true},
                        new SelectListItem {Text = "Provider Name 2", Value = "2"}
                    }, "Select Provider Name", new {@class = "dashInp"})
                </div>
            </div>
            <div class="clearfix"></div><br />
            @*<div class="dashfrm-L">
                @Html.TextBox("fileName-filter", "", new { @placeholder = "FileDate", @class = "dashInp form-control input-filter", @search_index = "1" })
            </div>
            <div class="dashfrm-M">
                @Html.TextBox("fileStatus-filter", "", new { @placeholder = "FileName", @class = "dashInp form-control input-filter", @search_index = "2" })
            </div>
            <div class="dashfrm-M">
                @Html.TextBox("submitter-filter", "", new { @placeholder = "Source", @class = " dashInp form-control input-filter", @search_index = "12" })
            </div>
            <div class="dashfrm-R">
                @Html.TextBox("reciever-filter", "", new { @placeholder = "Claim Status", @class = "dashInp form-control input-filter", @search_index = "13" })
            </div>
            <div class="clearfix"></div>*@

            <div class="dashfrm-L1">
                <label class="lblTxtCountHeader"> Total Files </label>
                <br /><br />
                @*<label class="lblCount" style="color:#20a8d8;"> @FileCount </label>*@
                <a class="lblCount" style="color:#20a8d8;" href="/Claims/FileClaimDetails?sSelectedDate=&sMenu=total">@FileCount</a>
            </div>
            <div class="dashfrm-L1">
                <label class="lblTxtCountHeader"> Dup Files </label>
                <br /><br />
                <label class="lblCount" style="color:#20a8d8;"> - </label>
            </div>
            <div class="dashfrm-L1">
                <label class="lblTxtCountHeader"> 999 </label>
                <br /><br />
                <label class="lblCount" style="color:#2ca058;"> @Count999 </label>
            </div>
            <div class="dashfrm-L1">
                <label class="lblTxtCountHeader"> 277 CA </label>
                <br /><br />
                <label class="lblCount" style="color:#ff0000;"> 0 </label>
            </div>
            <div class="dashfrm-L1">
                @*<label class="lblTxtCountHeader"> DocuStream Mismatch </label>
                <br /><br />
                <label class="lblCount" style="color:#ff0000;"> 17 </label>*@
            </div>
            <div class="dashfrm-L2">
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="dashBx">
            <div class="table-responsive" id="rootId" style="margin-top:5px;">
                <table id="tblId1" class="table table-striped">
                    <thead>
                        <tr style="background: #139DC9;color: white;">
                            <th style="text-align:left;">File Name<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">Submitted<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">InBiztalk<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">Rejected PreProcess<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">Error in PreProcess<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">Pending in Preprocess<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                            <th style="text-align:left;">In Facets<img src="~/Images/search_3_16.ico" style="float: right" /></th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td style="text-align:left;">Totals</td>
                            <td style="text-align:right;">@Model.SubTotal</td>
                            <td style="text-align:right;">@Model.InBizstockTotal</td>
                            <td style="text-align:right;">@Model.RejTotal</td>
                            <td style="text-align:right;">@Model.errTotal</td>
                            <td style="text-align:right;">@Model.PenTotal</td>
                            <td style="text-align:right;">@Model.VeriTotal</td>
                        </tr>
                        @if (Model.listitem.Count > 0)
                        {
                            foreach (var item in Model.listitem)
                            {
                                <tr>
                                    <td style="text-align:left;">@item.filename</td>
                                    <td style="text-align:right;">@item.Submitted</td>
                                    <td style="text-align:right;">@item.InBizstock</td>
                                    <td style="text-align:right;">@item.Rejected</td>
                                    <td style="text-align:right;">@item.Error</td>
                                    <td style="text-align:right;">@item.Pending</td>
                                    <td style="text-align:right;">@item.Verified</td>
                                </tr>
                            }
                        }
                        else
                        {
                            <tr><td style="border:none; text-align:left;">No records found</td></tr>

                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>
